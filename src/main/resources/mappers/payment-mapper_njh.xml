<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="paymentMapper_njh">
	<insert id="insertPayment" parameterType="Reservation">
		INSERT INTO PAYMENT
		VALUES('P'||SEQ_PNO.NEXTVAL,'R'||SEQ_RNO.CURRVAL,
				DEFAULT,DEFAULT,DEFAULT,DEFAULT,'Y',DEFAULT)
	</insert>
</mapper>